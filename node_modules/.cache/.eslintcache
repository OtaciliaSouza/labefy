efani-Moreira/modulo2/semana7/projeto-labefy/labefy/src/components/styles/StyledDetails.js",[],"/home/otacilia/outras/Stefani-Moreira/modulo2/semana7/projeto-labefy/labefy/src/components/styles/StyledAddTracks.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/index.js",[],["148","149"],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/App.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/StyledApp.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/Home.js",["150"],"import React from \"react\";\nimport axios from \"axios\";\nimport HomePlaylists from \"./HomePlaylists\";\nimport UserPlaylists from \"./UserPlaylists\";\nimport PlaylistsDetails from \"./PlaylistsDetails\";\nimport CreatePlaylists from \"./CreatePlaylists\";\nimport Sidebar from \"./Sidebar\";\nimport { HomePage, MainContainer } from \"./styles/StyledHome\";\n\nconst urlApi =\n  \"https://us-central1-labenu-apis.cloudfunctions.net/labefy/playlists\";\n\nconst headers = {\n  headers: {\n    Authorization: \"otacilia-souza-gebru\",\n  },\n};\n\nexport default class Home extends React.Component {\n  state = {\n    playlists: [],\n    playlistName: \"\",\n    playlistId: \"\",\n    tracks: [],\n    page: \"home\",\n  };\n\n  componentDidMount() {\n    this.getAllPlaylists();\n  }\n\n  getAllPlaylists = async () => {\n    try {\n      const res = await axios.get(urlApi, headers);\n      this.setState({ playlists: res.data.result.list });\n    } catch (err) {\n      console.log(err.message);\n    }\n  };\n\n  deletePlaylist = async (id) => {\n    const question = window.confirm(\n      \"Tem certeza que deseja deletar a playlist selecionada?\"\n    );\n    if (question) {\n      try {\n        const res = await axios.delete(`${urlApi}/${id}`, headers);\n        alert(\"Playlist deletada\");\n        this.getAllPlaylists();\n        this.pagePlaylists();\n      } catch (err) {\n        console.log(err.message);\n      }\n    }\n  };\n\n  getPlaylistTracks = async (id, name) => {\n    try {\n      const res = await axios.get(`${urlApi}/${id}/tracks`, headers);\n      this.setState({\n        tracks: res.data.result.tracks,\n        playlistName: name,\n        playlistId: id,\n        page: this.state.page,\n      });\n      console.log(this.state.tracks);\n    } catch (err) {\n      console.log(err.response.data.message);\n    }\n  };\n\n  renderPages = () => {\n    switch (this.state.page) {\n      case \"home\":\n        return <HomePlaylists />;\n      case \"playlists\":\n        return (\n          <UserPlaylists\n            pagePlaylists={this.pagePlaylists}\n            pageDetails={this.pagePlaylistsDetails}\n            pageAdd={this.pageAddTracks}\n            playlistName={this.state.playlistName}\n            playlistId={this.state.playlistId}\n            tracks={this.state.tracks}\n            getPlaylistTracks={this.getPlaylistTracks}\n            getAllPlaylists={this.getAllPlaylists}\n            playlists={this.state.playlists}\n            deletePlaylist={this.deletePlaylist}\n          />\n        );\n      case \"details\":\n        return (\n          <PlaylistsDetails\n            pagePlaylists={this.pagePlaylists}\n            pageDetails={this.pagePlaylistsDetails}\n            pageAdd={this.pageAddTracks}\n            playlistName={this.state.playlistName}\n            playlistId={this.state.playlistId}\n            tracks={this.state.tracks}\n            getPlaylistTracks={this.getPlaylistTracks}\n            getAllPlaylists={this.getAllPlaylists}\n            deletePlaylist={this.deletePlaylist}\n          />\n        );\n      case \"create\":\n        return (\n          <CreatePlaylists\n            pagePlaylists={this.pagePlaylists}\n            pageDetails={this.pagePlaylistsDetails}\n            getAllPlaylists={this.getAllPlaylists}\n          />\n        );\n      default:\n        return;\n    }\n  };\n\n  pageHome = () => {\n    this.setState({ page: \"home\" });\n  };\n\n  pagePlaylists = () => {\n    this.setState({ page: \"playlists\" });\n  };\n\n  pagePlaylistsDetails = () => {\n    this.setState({ page: \"details\" });\n  };\n\n  pageCreatePlaylists = () => {\n    this.setState({ page: \"create\" });\n  };\n\n  pageAddTracks = () => {\n    this.setState({ page: \"add\" });\n  };\n\n  render() {\n    return (\n      <HomePage>\n        <Sidebar\n          pagePlaylists={this.pagePlaylists}\n          pageDetails={this.pagePlaylistsDetails}\n          pageCreate={this.pageCreatePlaylists}\n          getPlaylistTracks={this.getPlaylistTracks}\n          getAllPlaylists={this.getAllPlaylists}\n          playlists={this.state.playlists}\n          pageHome={this.pageHome}\n        />\n        <MainContainer>{this.renderPages()}</MainContainer>\n      </HomePage>\n    );\n  }\n}\n","/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/HomePlaylists.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/UserPlaylists.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/CreatePlaylists.js",["151","152"],"import React from \"react\";\nimport axios from \"axios\";\nimport { MainContainer, Text } from \"./styles/StyledCreatePlaylist\";\n\nconst urlApi =\n  \"https://us-central1-labenu-apis.cloudfunctions.net/labefy/playlists\";\n\nconst headers = {\n  headers: {\n    Authorization: \"otacilia-souza-gebru\",\n  },\n};\n\nexport default class CreatePlaylists extends React.Component {\n  state = {\n    playlist: \"\",\n  };\n\n  onChangePlaylistName = (event) => {\n    this.setState({ playlist: event.target.value });\n  };\n\n  createPlaylist = async () => {\n    const body = { name: this.state.playlist };\n    try {\n      const res = await axios.post(urlApi, body, headers);\n\n      alert(\"Playlist criada\");\n      this.setState({ playlist: \"\" });\n      this.props.getAllPlaylists();\n    } catch (err) {\n      alert(err.message);\n    }\n  };\n\n  render() {\n    return (\n      <div>\n        <MainContainer>\n          <h2>Crie sua nova playlist</h2>\n          <input\n            placeholder={\"Minha playlist\"}\n            value={this.state.playlist}\n            onChange={this.onChangePlaylistName}\n          />\n          <button onClick={this.createPlaylist}>Criar Playlist</button>\n          <button onClick={() => this.props.pagePlaylists()}>\n            Acessar Playlists\n          </button>\n        </MainContainer>\n        {/* Obs1: acesse a biblioteca e em seguida clique na palylist a qual deseja adicionar \n        mais uma musica */}\n       \n      </div>\n    );\n  }\n}\n","/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/PlaylistsDetails.js",["153"],"import React from \"react\";\nimport axios from \"axios\";\nimport AddTrack from \"./AddTracks\";\nimport LeftButton from \"./images/left-chevron.png\";\nimport {\n  PlaylistContainer,\n  TrackContainer,\n  TrackCard,\n  LeftButtonContainer,\n  TracksListContainer,\n  ButtonMainContainer1,\n  MainContainer2,\n} from \"./styles/StyledDetails\";\n\nconst urlApi =\n  \"https://us-central1-labenu-apis.cloudfunctions.net/labefy/playlists\";\n\nconst headers = {\n  headers: {\n    Authorization: \"otacilia-souza-gebru\",\n  },\n};\n\nexport default class PlaylistsDetails extends React.Component {\n  state = {\n    addTrackOpen: false,\n  };\n\n  openAddTrack = () => {\n    this.setState({\n      addTrackOpen: true,\n    });\n  };\n\n  closeAddTrack = (event) => {\n    this.setState({\n      addTrackOpen: false,\n    });\n  };\n\n  componentDidMount() {\n    this.props.getAllPlaylists();\n    this.props.getPlaylistTracks();\n  }\n\n  randomNumber() {\n    return Math.floor(Math.random() * 100);\n  }\n\n  removeTrackFromPlaylist = async (id) => {\n    const question = window.confirm(\n      \"Tem certeza que deseja deletar esta a música escolhida?\"\n    );\n    if (question) {\n      try {\n        const res = await axios.delete(\n          `${urlApi}/${this.props.playlistId}/tracks/${id}`,\n          headers\n        );\n        alert(\"Música deletada\");\n        this.props.getPlaylistTracks(this.props.playlistId);\n      } catch (err) {\n        alert(err.res.data.message);\n      }\n    }\n  };\n\n  render() {\n    const showTracks = this.props.tracks.map((tracks) => {\n      return (\n        <TrackContainer key={tracks.id}>\n          <TrackCard\n            src={tracks.url}\n            allowfullscreen=\"\"\n            allow=\"autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture\"\n            title=\"Song\"\n          />\n          <button onClick={() => this.removeTrackFromPlaylist(tracks.id)}>\n            Deletar faixa\n          </button>\n        </TrackContainer>\n      );\n    });\n\n    return (\n      <div>\n        {this.state.addTrackOpen && (\n          <div>\n            <AddTrack\n              getPlaylistTracks={this.props.getPlaylistTracks}\n              playlistId={this.props.playlistId}\n              closeAddPage={this.closeAddTrack}\n            />\n          </div>\n        )}\n        <div>\n          <LeftButtonContainer>\n            <img\n              src={LeftButton}\n              alt=\"Ícone de flecha\"\n              onClick={this.props.pagePlaylists}\n            />\n          </LeftButtonContainer>\n          <PlaylistContainer>\n            <img\n              src={`https://picsum.photos/200/200/?a=${this.randomNumber()}`}\n              alt=\"Imagem\"\n            />\n            <li>{this.props.playlistName}</li>\n            <details>\n              <summary>...</summary>\n              <button\n                onClick={() => this.props.deletePlaylist(this.props.playlistId)}\n              >\n                Apagar\n              </button>\n            </details>\n          </PlaylistContainer>\n        </div>\n\n        {this.props.tracks.length === 0 && (\n          <MainContainer2>\n            <h4>Sua playlist está vazia 😕</h4>\n            <div>\n              <ButtonMainContainer1 onClick={this.openAddTrack}>\n                Adicionar Musica\n              </ButtonMainContainer1>\n            </div>\n          </MainContainer2>\n        )}\n        {this.props.tracks.length > 0 && (\n          <div>\n            <div>\n              <ButtonMainContainer1 onClick={this.openAddTrack}>\n                Adicionar Musica\n              </ButtonMainContainer1>\n            </div>\n            <TracksListContainer>{showTracks}</TracksListContainer>\n          </div>\n        )}\n      </div>\n    );\n  }\n}\n","/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/Sidebar.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/styles/StyledHome.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/styles/StyledHomePlaylists.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/styles/StyledCreatePlaylist.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/styles/StyledUserPlaylists.js",[],"/home/otacilia/gebru-Otacilia-Souza/projeto-labefy/src/components/AddTracks.js",["154","155"],"import React from \"react\";\nimport axios from \"axios\";\nimport {\n  AddContainer,\n  ContainerItems,\n  MainContainer,\n  Text,\n} from \"./styles/StyledAddTracks\";\n\nconst urlApi =\n  \"https://us-central1-labenu-apis.cloudfunctions.net/labefy/playlists\";\n\nconst headers = {\n  headers: {\n    Authorization: \"otacilia-souza-gebru\",\n  },\n};\n\nexport default class AddTracks extends React.Component {\n  state = {\n    name: \"\",\n    artist: \"\",\n    urlTrack: \"\",\n  };\n\n  onChangeName = (event) => {\n    this.setState({ name: event.target.value });\n  };\n\n  onChangeArtist = (event) => {\n    this.setState({ artist: event.target.value });\n  };\n  onChangeURL = (event) => {\n    this.setState({ urlTrack: event.target.value });\n  };\n\n  addTrackToPlaylist = async (id) => {\n    const body = {\n      name: this.state.name,\n      artist: th